{% if inventory_hostname != mysql_host -%}
25 * * * *  backup  mysqldump agama > /home/backup/mysql/agama.sql
25 * * * 0  backup  duplicity --no-encryption full /home/backup/mysql/ rsync://{{ backup_ssh }}.{{startup}}//home/pongraczfarkas/

25 * * * 1-6  backup  duplicity --no-encryption incremental /home/backup/mysql/ rsync://{{ backup_ssh }}.{{startup}}//home/pongraczfarkas/
25 * * * 1-6  backup  duplicity remove-older-than 1M --force rsync://{{ backup_ssh }}.{{startup}}//home/pongraczfarkas/influxdb
{% endif -%}
