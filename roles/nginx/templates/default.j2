

server {

	listen 80 default_server;
	server_name _;

	location / {
		uwsgi_pass localhost:5000;
		include uwsgi_params;
	}
{% for sites in reverse_proxy %}
	location {{sites['location']}} {
{% if 'extra' in sites %}
		{{sites['extra']}}
{% endif %}
		proxy_pass http://{{groups[sites['name']][0]}}:{{sites['port']}};
	}
{% endfor %}	

	
}
